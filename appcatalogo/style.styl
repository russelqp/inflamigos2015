@import "normalize.styl"
@import "../css/style-catalogo.styl"
@import "../css/style-item.styl"

@charset "utf-8"
*
	margin 0
	padding 0

body
	background-color #fff
	color #09f
	font-family 'Noto Sans', sans-serif
	font-size 16px
	margin 0 auto
	padding 0

img
	border 0

h1,h2,h3,h4,h5,h6
	margin 0 auto
	font-family 'Wendy One', sans-serif
	text-shadow 1px 1px 1px rgba(60,60,60,0.5)
	text-align center
	font-weight 60

h1
	color #FC3
	font-size 3.0em
	text-align center

h2
	text-align center

h3
	font-size 1.4em
	color #fff
	width 95%
	display block
	padding 5px

article
	figure
		.menu_inflables
			h4
				color #fefefe
				font-size 1em
					text-align center
					background-color #C4D000
					font-family Arial

h4
	color #333
	font-size 1em

h5
	font-size 0.8em
	text-align center
	color #666

h6
	font-size 1.0em

a
	text-decoration none

section, article, aside, nav, .boletin, .boletin p, .menu_inflables ul li, .menu_inflables imghover, .lista-imagenes ul, .lista-imagenes li, #ubicacion
	box-sizing border-box
	margin 2px auto
	list-style-position inside
	list-style-type disc
	color #F30
	padding 0 10px

header
	display block
	margin 5px auto
	width 100%
	padding 5px 0
	text-align center

header p
	width 80%

header #logo
	border 0
	display inline-block
	width 10%

header #inflabletop
	border 0
	display inline-block
	width 10%

header #info
	display inline-block
	width 78%
	text-align center
	margin 0 auto


header #logo img, header #inflabletop img
	width 100%

header ol
	list-style none
	text-decoration none
	padding 0
	margin 0 auto


article
	color #121212
	margin 0 auto
	padding 0
	width 100%

article p
	font-size 1.1em
	margin 15px auto
	width 85%
	padding 10px

footer
	background-color #FF4900
	bottom 0%
	margin 0 auto
	padding 0
	position relative
	text-align center
	z-index 1
	width 100%

section
	background-color #fffefe
	box-shadow 0 0 3px 2px rgba(60,60,60,0.8)
	display block
	margin 0 auto
	width 100%
	padding 10px 0

#item h3
	background-color #f30


#container
	margin 5px auto
	width 100%
	padding 0
	position relative
	text-align justify
	z-index 3

.bnt_ir
	width 90%
	display block
	background-color #FE4900
	margin 2px auto
	border-radius 2px

.btn_ir a:link
	color #ccc 

.bnt_ir:hover
	background-color #6ff

.credits
	margin 0 auto
	color #ddd
	text-align center

.btn_volver
	box-shadow 0 1px 0 0 #fff6af
	background: -webkit-gradient(linear,left top,left bottom,color-stop(.05,#ffec64),color-stop(1,#ffab23));
	background: -moz-linear-gradient(top,#ffec64 5%,#ffab23 100%);
	background: -webkit-linear-gradient(top,#ffec64 5%,#ffab23 100%);
	background: -o-linear-gradient(top,#ffec64 5%,#ffab23 100%);
	background: -ms-linear-gradient(top,#ffec64 5%,#ffab23 100%);
	background: linear-gradient(to bottom,#ffec64 5%,#ffab23 100%);
	background-color #ffec64
	border-radius 6px
	border 1px solid #fa2
	display inline-block
	cursor pointer
	color #333
	font-family arial
	font-size 15px
	font-weight bold
	padding 6px 24px
	text-decoration none
	text-shadow 0 1px 0 #fe6

.btn_volver:hover
	background linear-gradient(to bottom,#ffab23 5%,#ffec64 100%)
	background-color #ffab23

.btn_volver:active
	position relative
	top 1px


.credits a:link
	text-decoration none
	padding 0
	margin-top 0

.credits p
	margin 0 auto
	padding 10px 0
	text-align center
	width 80%

.lista
	display none

figure
	border-radius 3px
	background-color #Fefefe
	color #333
	padding 5px
	margin 10px 5px
	width 29%
	display inline-block
	vertical-align top
	transition 0.2s ease-out
	text-align center
	box-shadow 0 0 2px 1px rgba(46, 49, 50, 0.3)

figure a 
	display block
	border-radius 3px
	color #FFF

figure img
	width 100%
	padding 0
	margin 0
	border-radius 5px

figure:hover
	transform scale(1.02,1.02);

.datos
	display inline-block
	vertical-align top
	width 30%
	padding 2%


.cat
	margin 5px auto
	width 100%

.cat
	margin 0 auto

.articborder
	border-bottom 1px dashed #09F
	margin 10px

.fltrt
 	float right
	margin-left 8px

.fltlft
 	float left
	margin-right 8px

.clearfloat
	clear both

.lista-imagenes
	display inline-block
	vertical-align top
	width 60%
	padding 2%
	text-align center
	margin 0 auto

.lista-imagenes h4
	text-align left
	margin 10px auto
	background none

.lista-imagenes ul
	padding 0
	margin 0
	text-align left

.lista-imagenes li
	display inline-block
	list-style-type none
	margin 2px
	transition all 0.8s ease
	width 30%

.lista-imagenes li img
	width 100%
	padding 0
	box-shadow 3px 3px 2px rgba(46, 49, 50, 0.3)
	border 1px solid #D30
	border-radius 5px

.lista-imagenes li img:hover
	transform scale(1.08)

.datos
	display inline-block
	vertical-align top
	width 30%
	padding 2%

#openModal, .btn_reservar
	display none

.uvideo
	margin 10px auto
	padding-top 10px
	text-align center
	width 80%

@import "responsive.styl"