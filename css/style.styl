@charset "utf-8"
@import "normalize.styl"
@import "iconos.styl"
@import "style-catalogo.styl"
@import "style-item.styl"
@import "book-now.styl"
@import "style-landings.styl"

body
	background-color #eceff1
	color #FFF
	font-family 'Noto Sans', sans-serif
	font-size 16px
	margin 0 auto
	padding 0

img
	border 0
	padding 0

h1,h2,h3,h4,h5,h6
	margin 0 auto
	font-family 'Wendy One', sans-serif
	text-shadow 1px 1px 1px rgba(60,60,60,0.5)
	text-align center

h1
	color #0CF
	font-size 3em
	display inline-block
	margin 0 auto
	text-shadow: 0px 4px 3px rgba(0,0,0,0.4),
             0px 8px 13px rgba(0,0,0,0.1),
             0px 18px 23px rgba(0,0,0,0.1);


h1 img
		float left

h2
	text-align center

h3
	font-size 2.5em
	padding 5px 10px
	color #fff
	background-color #FB3259
	margin 10px auto
	display table
	box-sizing border-box
	text-align center
	box-shadow 0 8px 6px -6px black

h4
	color #fff
	background-color #fc3
	display table
	font-size 1.8em
	padding 5px 10px
	margin 10px auto
	border-radius 2px
	box-sizing border-box
	text-align center
	box-shadow 0 8px 6px -6px black

h5
	font-size 1.5em

h6
	font-size 1.0em

a
	text-decoration none

section, article, aside, .lista, .lista img, .lista ol, .lista li, .boletin, .boletin p, .menu_inflables ul li, #ubicacion
	box-sizing border-box
	margin 0 auto
	color #F30
	padding 0 10px

header
	display block
	margin 0 auto
	width 100%
	padding 0
	text-align center

	h1 img
			padding 0
			margin 0
			vertical-align top
			display inline-block
span
	color #FF5C00
	font-size 1.2em
	font-family: Tahoma
	text-shadow: none

p
	width 80%

#top_cintilla
	padding 5px auto
	text-align center
	background-color #FB3259
	margin 0 auto
	width 100%

#logo, #info
	text-align center
	display inline-block

#logo
	width 20%
	img
		display block

#logo-min
	display none

#info
		text-align center
		margin 0 auto
		vertical-align bottom

#info h1 a:visited
				color #FC3

	nav
		margin 0 auto
		text-align center
		width 100%
		display block
		padding 0
		border-bottom 1px solid #BBA506
		box-sizing border-box

	nav ul
			width 100%
			padding 0
			background-color #ffbc00
			font-size  0
			display inline-block
			border-top 2px solid #ffdc73

	nav ul li
			background-color #FFCC00
			color #000
			display inline-block
			font-size 16px
			height 45px
			line-height 45px
			margin 0
			text-align left
			width 18%


	nav ul li a
				display block
				color #fff
				text-shadow 0 1px 0 #aaa
				transition all 1s ease
				height 100%
				padding-left 5px
				font-weight bold
				font-family "Arial Black", Gadget, sans-serif

	nav ul li a:hover
				background-color #e9f906
				text-align center
				color #ee0000

	nav ul li a:visited
				border 1px solid #D30

#boletin_video article, aside
							display inline-block

#boletin_video blockquote
					color #fff

article
	color #121212
	margin 0 auto
	padding 0px
	text-align center
	vertical-align top
	width 100%

aside
	width 30%
	margin 0 15px 0 auto
	text-align center
	padding 0px
	color #FFF

article .lista li p
					width 75%
					margin 5px auto
					text-align justify
					padding 0
					color #39F

article p
			font-size 1.4em
			margin 5px auto
			width 85%

#banner_video
			width 100%
			padding 5px
			margin 0 auto
			display block
			vertical-align top
			background-color #FFF

#boletin_video
			width 100%
			backgrround-color #f00
			margin 10px auto
			pading 0

#welcome
	width 100%
	text-align center
	background-color #fff
	margin 10px auto

#welcome-servicios
			margin 10px auto

#welcome-servicios article
						width 100%
						padding 0
						margin 0

#welcome #wowslider-container1
					width 67%
					margin 0 auto
					display inline-block
					vertical-align top
					background rgba(255,255,255,0.2)
					position relative
					text-align center

#welcome #wowslider-container1  img
									width 100%
									padding 0
									margin 0
									max-width 600px
.miniserv
	border-radius 5px
	border 5px solid #eee
	background-color #fff
	color #333
	padding 10px
	margin 2px 5px
	width 29%
	display inline-block
	vertical-align top
	transition all .5s ease
	text-align center
	box-shadow 0 0 2px 2px rgba(46, 49, 50, 0.3)

	p
		width 80%
		margin 0 auto
		padding 0
		text-align right

	h4
		background none
		color #eb0038
		text-align center

.miniserv .compartir
			width 40%
			margin 0 2px
			padding 0
			display inline-block
			vertical-align top

.miniserv ul
			padding 0
			list-style-type none
			text-align right

.miniserv ul li
				display inline-block
				vertical-align top
				padding 0
				width 20%
				margin 0 2px

.btn_red
		margin 0 auto
		display block
		vertical-align top
		font-size 1.7em
		color #D20033
		text-shadow 0 0 2px rgba(60,60,60,0.5)
		transition all 0.2s ease
		background-color #d3d3d3
		border none
		border-bottom 3px solid #808080
		border-radius 3px
		box-sizing border-box
		color #fff !important
		cursor pointer
		font-family 'Lato',sans-serif
		font-weight bold
		height 35px
		line-height 32px
		text-decoration none
		user-select none
		background-color #7dcd40
		border-color #6dae33
		text-align center
		width 90%

.miniserv img
			width 100%
			margin 0

.miniserv:hover
	transform scale(1.02)
	border 5px solid orange

footer
	background-color #fb3259
	bottom 0%
	margin 10px auto 0 auto
	padding 0
	position relative
	text-align center
	width 100%

.btn_reservar
			width 80%
			background red
			padding 8px 14px 10px
			border 1px solid red
			cursor pointer
			font-size 1.5em
			font-family Oswald, sans-serif
			letter-spacing .1em
			text-shadow 0 -1px 0px rgba(0, 0, 0, 0.3)
			color #fff
			box-shadow inset 0px 1px 0px rgba(255,100,255,0.5), 0px 5px 0px 0px rgba(255,160,200,0.9), 0px 10px 5px #999
			border-radius 10px
			display inline


.btn_reservar:hover, .btn_reservar:focus
								color #dfe7ea
								box-shadow inset 0px 1px 0px rgba(255,255,255,0.5), 0px 2px 0px 0px rgba(255,255,255,0.5), 0px 2px 5px #99

.submenu a
			display block
			text-decoration none
			height 25px
			padding 5px 5px 0 5px
			transition all 0.5s ease
			width 100%

.submenu a:hover
			display block
			background-color #EE3
			width 100%

blockquote
	font-size 18px
	color #fff
	font-weight 600
	margin 10px auto
	width 80%
	text-align center
	text-shadow -1px -1px 1px rgba(60,60,60,0.5)
	background-color #0cf

section
	margin 0 auto
	width 100%
	text-align center

#bk-home
	border-right 1px solid #BF6230

#bk-brincolines
	border-left 1px solid #FFDC73
	border-right 1px solid #BF6230

#bk-servicios
	border-left 1px solid #FFDC73
	border-right 1px solid #BF6230

#bk-faq
	border-left 1px solid #FFDC73
	border-right 1px solid #BF6230

#bk-contacto
	border-left 1px solid #FFDC73
	border-right 1px solid #BF6230

#bk-blog
	border-left 1px solid #FFDC73

#mapa
	width 80%
	height 200px
	margin 10px auto
	display block
	border 1px solid #F33
	padding 0

#background
	box-sizing border-box
	height 100%
	margin 0
	padding 0
	position absolute
	top 0%
	width 100%
	z-index 0

#container
	margin 10px auto
	max-width 1400px
	padding 5px
	position relative
	text-align center
	background-color #fefefe
	box-shadow  0px 2px 5px 0px rgba(0,0,0,0.15)


#item
	border 1px solid #cdcdcd
	width 96%
	margin 5px auto
	background rgba(250,250,250,1)
	border-radius 5px

	figure
		width 60%
		margin 0
		padding 5px
		vertical-align top
		display inline-block
		border 1px solid #cdcdcd
		border-radius 5px
		background-color #efefef

		figcaption
			border-top 1px solid #ddd

		img
			max-width 500px
			width 100%
			padding 0
			margin 5px

		p
			text-align left


#contact-area
	background-color #fefefe
	border 2px dashed #fff
	border-radius 5px
	margin 10px auto
	width 60%
	padding 20px 25px
	text-align center
	box-shadow 0 0 1em #ccc

	strong
		text-align right

	p
		color #121212
		text-align left

	h4
		width 100%
		padding 0
		margin 0

	span
		font-size 1.2em
		margin 10px auto
		color #09f
		text-align left
		width 80%
		font-weight bold

	.nombre, .telefono, .email, .phone, textarea
		border 1px dashed #0cf
		color #0cF
		font-family Helvetica, sans-serif
		font-size 1.8em
		width 95%
		margin 10px auto
		padding 10px 0 10px 10px

	.opinion
		border 1px dashed #FFF

	textarea:focus, input:focus
		border 1px solid #0cf
		background-color #D7F7F3

	input.submit-button
		background-color #FC3
		border-radius 5px
		font-family Helvetica, sans-serif
		font-size 2.0em
		font-weight bolder
		height 70px
		margin 0 auto
		text-align center
		width 80%
		transition .5s all ease
		color #FFF

	input.submit-button:hover
		color #fff
		font-size 1.8em
		background-color #D6AB2B

	textarea:placeholder
		color #09f

.btn_activo
	background-color #fb3259
	border-left 0

.submenu
	a
		link
			color #09F

.ayudaonline
	width 100%
	height 40px
	margin 0
	color #FC3
	padding 0
	text-align right
	background-image url(imagenes/ayudaonline.png)
	background-position right
	background-repeat no-repeat
	margin-right 20px
	padding-right 50px

.lista
	margin 20px auto
	width 100%
	padding 10px
	text-align center

.lista .img-list, .lista ol
	padding 0
	margin 0
	display inline-block
	vertical-align top

.lista .img-list
	width 15%

.lista .img-list img
	width 100%
	padding 0

.lista ol
	width 75%
	list-style none
	background-color #FC3
	border-radius 5px
	border 2px solid #F90
	padding 10px 0

.lista ol li
		color #f80012
		text-align left
		font-size 1.2em

.ubicacion
	background-color #F90
	margin 0 auto
	padding 0
	width 948px

.ubicacion p
	margin  auto
	text-align left

.uvideo
	margin 10px auto
	padding-top 10px
	text-align center
	width 80%

.uvideo a link
	color #03C

.banner
	width 650px
	margin 0 auto

#social-holder
	width 100%
	margin 5px auto
	text-align center
	padding 2px 0
	background-color #FB0
	border-radius 5px
	display inside-block

#social-holder ul li
	margin 2px
	padding 0
	display inline-block

#social-holder ul li a
	display block
	padding 0
	font-size 1.8em
	color #D20033
	text-shadow 0 0 2px rgba(60,60,60,0.5)
	transition all 0.5s ease

#social-holder ul li a:hover
	color #00AF64
	transform scale(1.1)

.fb-like-box
		margin 10px auto
		text-align center
		background-color #F60
		border 0

.boletin
	background-color #FB0
	border-radius 5px
	border 0
	border 1px solid #FD3
	display inline-block
	margin 0 auto
	padding 5px 2px
	text-align center
	vertical-align top
	width 100%

.boletin p
	width 90%
	background-color #00AF64
	padding 5px
	border-radius 3px
	font-size 1.0em
	color #fff
	margin 5px auto

.boletin .nombre-bol
	width 96%
	margin 0 auto
	padding 0
	height 30px
	border-top 1px solid #09f
	border-right 1px solid #09f

.boletin .email-bol
	width 96%
	margin 0 auto
	padding 2px
	border-top 1px solid #09f
	border-right 1px solid #09f
	color #FFA400

.boletin .btn-bol
	width 90%
	background-color #DDD
	margin 0 auto
	padding 0
	height 35px
	border-radius 5px
	border 1px solid #CCC
	color #555
	box-shadow 0 0 5px rgba(60,60,60,0.5)
	cursor pointer
	transition all 1s ease

.boletin .btn-bol:hover
	background-color #00BF32
	color #fff

.boletin #tlemail
	width 90%
	font-size 1.2em
	color #09f

.submenu
	background-color #FFFC00
	color #9C0
	padding 0
	text-align center
	margin 0 auto

.submenu ul
	margin 0 auto
	width 80%
	padding 0

.submenu li
	border-left 2px solid #FF9
	border-right 2px solid #F90
	display inline-block
	vertical-align top
	height 30px
	list-style-type none
	margin 0
	padding 0

.submenu li.nb-left
	border-left 0

.submenu li.nb-right
	border-right 0


.credits
	margin 0 auto
	color #FFF
	text-align center


.credits alink
	text-decoration none
	padding 0
	margin-top 0

.credits p
	margin 0 auto
	padding 10px 0
	text-align center
	width 80%


.cat
	margin 5px auto
	width 100%

.cat
	margin 0 auto

.articborder
	border-bottom 1px dashed #09F
	margin 10px

#mapadesitio
	margin 0 auto
	width 80%
	padding 0

	h5
		text-align left

#mapadesitio li
	color #F60
	font-size 1.2em
	list-style disc
	margin-left 20px
	text-align left

#mapadesitio a, #mapadesitio a:link, #mapadesitio a:visited
	text-decoration underline

.fltrt
 	float right
	margin-left 8px

.fltlft
 	float left
	margin-right 8px

.clearfloat
	clear both

@import "responsive.styl"
@import "style-reservacion.styl"